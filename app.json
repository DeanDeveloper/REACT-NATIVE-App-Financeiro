{
  "expo": {
    "name": "Financentral",
    "description": "App Finance",
    "slug": "Financentral",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon":"./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode":"contain",
      "backgroundColor": "#8800ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "platforms" : [
      "ios",
      "android"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],


    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mycompany.appFinance",
      "adaptiveIcon": {
        "backgroundColor":"#8800ff",
        "foregroundImage":"./assets/icon.png"
      }
    }
  }
}